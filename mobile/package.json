{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "env:dev": "node scripts/switch-env.mjs dev",
    "env:prod": "node scripts/switch-env.mjs prod",
    "start": "npx expo start",
    "start:clean": "npx expo start -c",
    "start:dev": "npm run env:dev && npx expo start",
    "start:dev:clean": "npm run env:dev && npx expo start -c",
    "start:prod": "npm run env:prod && npx expo start --no-dev --minify",
    "start:prod:clean": "npm run env:prod && npx expo start --no-dev --minify -c",
    "android:dev": "npm run env:dev && npx expo run:android",
    "ios:dev": "npm run env:dev && npx expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "axios": "^1.12.2",
    "expo": "54.0.13",
    "expo-asset": "~12.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-router": "~6.0.12",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "jwt-decode": "^4.0.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1"
  },
  "private": true,
  "devDependencies": {
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.1",
    "react-native-dotenv": "^3.4.11"
  }
}
